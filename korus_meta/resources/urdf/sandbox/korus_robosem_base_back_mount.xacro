<?xml version="1.0" ?>
  <robot xmlns:xacro="http://www.ros.org/wiki/xacro">
    
	  <!-- ************************ Properties ************************** -->
	  <xacro:property name="pi" value="3.1415926535897931" />
	  <xacro:property name="material_white">
	    <material name="white">
	      <color rgba="1 1 1 1"/>
	    </material>
	  </xacro:property>
	  <xacro:property name="material_orange">
	    <material name="orange">
	      <color rgba="1.0 0.7 0.0 1"/>
	    </material>
	  </xacro:property>
	  <xacro:property name="material_red">
	    <material name="red">
	      <color rgba="1.0 0.0 0.0 1"/>
	    </material>
	  </xacro:property>
	  <xacro:property name="material_grey_blue">
	    <material name="grey_blue">
	      <color rgba="0.4 0.4 1.0 1"/>
	    </material>
	  </xacro:property>
	  <xacro:property name="material_blue">
	    <material name="blue">
	      <color rgba="0.0 0.0 1.0 1"/>
	    </material>
	  </xacro:property>
	  <xacro:property name="material_green">
	    <material name="green">
	      <color rgba="0 .8 0 1"/>
	    </material>
	  </xacro:property>
	  <xacro:property name="material_grey">
	    <material name="grey">
	      <color rgba="0.7 0.7 0.7 1.0"/>
	    </material>
	  </xacro:property>
	  <xacro:property name="material_black">
	    <material name="black">
	      <color rgba="0.1 0.1 0.1 1"/>
	    </material>
	  </xacro:property>
    <xacro:property name="wheel_bias" value="0.3" />
    <xacro:property name="wheel_radius" value="0.075" />
    <xacro:property name="caster_wheel_bias" value="0.2" />
    <xacro:property name="caster_wheel_radius" value="0.040" />	  
	  
    <!-- ************************** Wheel macro ************************** -->
	  <xacro:macro name="wheel" params="name length radius off_x off_y off_z type">
	    <link name="${name}">
	      <visual>
	        <geometry>
	          <cylinder length="${length}" radius="${radius}"/>
	        </geometry>
	        <origin rpy="0 ${pi/2} 0" xyz="0 0 0"/>
	        <material name="black">
	          <color rgba="0.1 0.1 0.1 1"/>
	        </material>
	      </visual>
	    </link>
	    <joint name="${name}" type="${type}">
	      <parent link="base_link"/>
	      <child link="${name}"/>
	      <origin rpy="0 0 ${pi/2}" xyz="${off_x} ${off_y} ${off_z}"/>
	    </joint>
	  </xacro:macro>
	  
    <!-- ************************** Robot macro ************************** -->
    <xacro:macro name="korus_robosem_base_back_mount" params="arm_link">
    
		  <!-- ************************** Base *************************** -->
		  <link name="base_footprint"></link>
		  <joint name="base_joint" type="fixed">
		    <parent link="base_footprint"/>
		    <child link="base_link"/>
		    <origin rpy="0 0 0" xyz="0.0 0.0 0.075"/>
		  </joint>
		  <link name="base_link">
		    <visual>
		      <origin rpy="0 0 0" xyz="0.0 0 0.0875"/>
		       <geometry>
		        <cylinder length="0.275" radius="0.250"/>
		      </geometry>
		      <xacro:insert_block name="material_white" />
		    </visual>
		  </link>
		  <xacro:wheel name="wheel_left" length="0.03" radius="${wheel_radius}" off_x="0.0" off_y="${wheel_bias/2}" off_z="0.0" type="continuous"/>
		  <xacro:wheel name="wheel_right" length="0.03" radius="${wheel_radius}" off_x="0.0" off_y="-${wheel_bias/2}" off_z="0.0" type="continuous"/>
		  <xacro:wheel name="caster_front_left" length="0.02" radius="${caster_wheel_radius}" off_x="0.18" off_y="${caster_wheel_bias/2}" off_z="-0.035" type="fixed"/>
		  <xacro:wheel name="caster_front_right" length="0.02" radius="${caster_wheel_radius}" off_x="0.18" off_y="-${caster_wheel_bias/2}" off_z="-0.035" type="fixed"/>
		  <xacro:wheel name="caster_back_left" length="0.02" radius="${caster_wheel_radius}" off_x="-0.18" off_y="${caster_wheel_bias/2}" off_z="-0.035" type="fixed"/>
		  <xacro:wheel name="caster_back_right" length="0.02" radius="${caster_wheel_radius}" off_x="-0.18" off_y="-${caster_wheel_bias/2}" off_z="-0.035" type="fixed"/>  
		
		  <!-- ************************** Body ************************** -->
		
		  <joint name="body" type="revolute">
		    <parent link="base_link"/>
		    <child link="body_link"/>
		    <origin rpy="0 0 0" xyz="0.0 0.0 0.250"/>
		    <axis xyz="0 0 1"/>
		    <limit effort="10" lower="-${pi}" upper="${pi}" velocity="0.013"/>
		  </joint>
		  <link name="body_link">
		    <visual>
		      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
		       <geometry>
		        <cylinder length="0.05" radius="0.25"/>
		      </geometry>
		      <xacro:insert_block name="material_white" />
		    </visual>  
		  </link>
		  
		  <!-- ************************** Torso *************************** -->
		  <joint name="torso_joint" type="fixed">
		    <parent link="body_link"/>
		    <child link="torso_link"/>
		    <origin xyz="-0.175 0.0 0.025" rpy="0 0 0"/>
		  </joint>
		  <link name="torso_link">
		    <visual>
		      <geometry>
		        <cylinder length="0.60" radius="0.075"/>
		      </geometry>
		      <origin rpy="0 0 0" xyz="0 0 0.3"/>
		      <xacro:insert_block name="material_white" />
		    </visual>
		  </link>
		  
		  <joint name="torso_lift_offset" type="fixed">
		    <parent link="torso_link"/>
		    <child link="torso_lift_offset_link"/>
		    <origin xyz="0 0 0.25" rpy="0 0 0"/>
		    <axis xyz="0 0 1"/>
		  </joint>
		  <link name="torso_lift_offset_link"></link>
		  
		  <joint name="torso_lift" type="prismatic">
		    <parent link="torso_lift_offset_link"/>
		    <child link="${arm_link}"/>
		    <origin xyz="0 0 0" rpy="0 0 ${pi}"/>
		    <axis xyz="0 0 1"/>
		    <limit effort="10" lower="-0.08" upper="0.08" velocity="0.5"/>
		  </joint>  
		  <link name="${arm_link}">
		<!--     <visual> -->
		<!--       <geometry> -->
		<!--         <box size="0.10 0.25 0.13"/> -->
		<!--       </geometry> -->
		<!--       <origin xyz="-0.05 0.0 0.0" rpy="0 0 0"/> -->
		<!--       <xacro:insert_block name="material_grey_blue" /> -->
		<!--     </visual> -->
		  </link>  
		
		  <!-- ************************** Neck ************************** -->
		  <joint name="neck_joint" type="fixed">
		    <parent link="torso_link"/>
		    <child link="neck_link"/>
		    <origin rpy="0 0 0" xyz="0  0.0 0.60"/>
		  </joint>
		  <link name="neck_link">
		    <visual>
		      <geometry>
		        <box size="0.300 0.20 0.04" />
		      </geometry>
		      <origin xyz="0.075 0.0 0.02" rpy="0 0 0" />
		      <xacro:insert_block name="material_white"/>
		    </visual>
		  </link>
		  
	  <!-- ************************** Head ************************** -->
	  <joint name="head_joint" type="revolute">
	    <parent link="neck_link"/>
	    <child link="head_link"/>
	    <origin rpy="0 0 0" xyz="0.175 0 0.04"/>
	    <axis xyz="0 0 1"/>
	    <limit effort="10" lower="-${pi/2}" upper="${pi/2}" velocity="0.013"/>
	  </joint>
	  <link name="head_link">
	    <visual>
	      <geometry>
	        <cylinder length="0.30" radius="0.05"/>
	      </geometry>
	      <origin rpy="0 0 0" xyz="0.0 0.0 0.15"/>
	      <xacro:insert_block name="material_white" />
	    </visual>  
	  </link>
	  
	  <!-- *********************** Asus Xtion ************************ -->
	  <joint name="asus_tilt_joint" type="revolute">
	    <parent link="head_link"/>
	    <child link="asus_link"/>
	    <origin rpy="0 0 -${pi/2}" xyz="0.05 0.0 0.20"/>
	    <axis xyz="1 0 0"/>
	    <limit effort="10" lower="-${pi/2}" upper="${pi/2}" velocity="0.013"/>
	  </joint>
	  <link name="asus_link">
	    <visual>
	      <geometry>
	        <box size="0.025 0.15 0.025" />
	      </geometry>
	      <origin rpy="0 0 ${-pi/2}" xyz="0.0 0.0 0.0"/>
	      <xacro:insert_block name="material_orange" />
	    </visual>
	  </link>
	
	  <!-- ************************ Tablet ************************* -->
	  <joint name="tablet_joint" type="fixed">
	    <parent link="head_link"/>
	    <child link="tablet_link"/>
	    <origin rpy="0 0 0" xyz="0.05 0.0 0.30"/>
	  </joint>
	  <link name="tablet_link">
	    <visual>
	      <geometry>
	        <box size="0.01 0.20 0.15" />
	      </geometry>
	      <origin rpy="0.0 -${pi/4} 0.0" xyz="0.0 0.0 0.0"/>
	      <xacro:insert_block name="material_black" />
	    </visual>  
	  </link>
  </xacro:macro>
</robot>