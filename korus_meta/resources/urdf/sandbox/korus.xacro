<?xml version="1.0" ?>

<!--  
  == USAGE ==
  
  - loaded into the kinematics node (move_arm.launch).
  - loaded into model viewer (view_model.launch).
  - loaded into robot viewer (view_robot.launch).
  
  == TODO ==
  
   - redo with real parameters later!
   - finish arm visuals
   
  == UPDATE ==
  
  If changing anything here, be sure to:
  
  - yaml/robot/*.yaml (joint name strings)
  - yaml/robot/joints.yaml (joint limits)
  - yaml/mobile_base/diff_drive_base.yaml (wheel name strings, bias, wheel_radius)
  - yaml/mechanisms/gripper,head,stereo_came_tilt.controller (joint name strings)
  - goo/*.yaml (actuator_name strings)
-->
<robot
  xmlns:xacro="http://www.ros.org/wiki/xacro"
  name="korus"
  >

  <!-- ************************** Xacro ************************** -->
  <xacro:property name="pi" value="3.1415926535897931" />
  <xacro:property name="material_white">
    <material name="white">
      <color rgba="1 1 1 1"/>
    </material>
  </xacro:property>
  <xacro:property name="material_orange">
    <material name="orange">
      <color rgba="1.0 0.7 0.0 1"/>
    </material>
  </xacro:property>
  <xacro:property name="material_red">
    <material name="orange">
      <color rgba="1.0 0.0 0.0 1"/>
    </material>
  </xacro:property>
  <xacro:property name="material_grey_blue">
    <material name="grey_blue">
      <color rgba="0.4 0.4 1.0 1"/>
    </material>
  </xacro:property>
  <xacro:property name="material_blue">
    <material name="blue">
      <color rgba="0.0 0.0 1.0 1"/>
    </material>
  </xacro:property>
  <xacro:property name="material_green">
    <material name="green">
      <color rgba="0 .8 0 1"/>
    </material>
  </xacro:property>
  <xacro:property name="material_black">
    <material name="black">
      <color rgba="0.1 0.1 0.1 1"/>
    </material>
  </xacro:property>
  <xacro:macro name="wheel" params="name length radius off_x off_y off_z type">
    <link name="${name}">
      <visual>
        <geometry>
          <cylinder length="${length}" radius="${radius}"/>
     	  </geometry>
        <origin rpy="0 ${pi/2} 0" xyz="0 0 0"/>
        <material name="black">
          <color rgba="0.1 0.1 0.1 1"/>
        </material>
      </visual>
    </link>
    <joint name="${name}" type="${type}">
      <parent link="base_link"/>
      <child link="${name}"/>
      <origin rpy="0 0 ${pi/2}" xyz="${off_x} ${off_y} ${off_z}"/>
    </joint>
  </xacro:macro>
  
  <!-- ************************** Base *************************** -->
  <link name="base_footprint"></link>
  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.075"/>
  </joint>
  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://korus_meta/resources/stl/base.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 -${pi/2}"/>
<!--      <geometry> -->
<!--         <box size="0.450 0.350 0.317" /> -->
<!--       </geometry> -->
<!--       <origin rpy="0 0 0" xyz="-0.130 0 0.120"/> -->
      <xacro:insert_block name="material_white" />
    </visual>
  </link>
  <xacro:property name="front_wheel_bias" value="0.3" />
  <xacro:property name="front_wheel_radius" value="0.075" />
  <xacro:property name="caster_wheel_bias" value="0.2" />
  <xacro:property name="caster_wheel_radius" value="0.040" />
  <xacro:wheel name="wheel_left" length="0.03" radius="${front_wheel_radius}" off_x="0.0" off_y="${front_wheel_bias/2}" off_z="0.0" type="continuous"/>
  <xacro:wheel name="wheel_right" length="0.03" radius="${front_wheel_radius}" off_x="0.0" off_y="-${front_wheel_bias/2}" off_z="0.0" type="continuous"/>
  <xacro:wheel name="caster_left" length="0.02" radius="${caster_wheel_radius}" off_x="-0.315" off_y="${caster_wheel_bias/2}" off_z="-0.035" type="fixed"/>
  <xacro:wheel name="caster_right" length="0.02" radius="${caster_wheel_radius}" off_x="-0.315" off_y="-${caster_wheel_bias/2}" off_z="-0.035" type="fixed"/>

  <!-- ************************** Body ************************** -->

  <joint name="body" type="fixed">
    <parent link="base_link"/>
    <child link="body_link"/>
    <origin rpy="0 0 0" xyz="-0.1725 0.0 0.282"/>
  </joint>
  <link name="body_link">
  </link>

  <!-- ************************** Head ************************** -->
  <joint name="head" type="fixed">
    <parent link="body_link"/>
    <child link="head_link"/>
    <!-- <origin rpy="0 0 0" xyz="0 0 0.4045"/> -->
    <origin rpy="0 0 0" xyz="0 0 0.3545"/>
  </joint>
  <link name="head_link">
    <visual>
      <geometry>
        <cylinder length="0.3145" radius="0.04"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.15725"/>
      <xacro:insert_block name="material_white"/>
    </visual>
  </link>
  
  <!-- *********************** StereoCam ************************ -->
  <joint name="stereo_cam_tilt" type="revolute">
    <parent link="head_link"/>
    <child link="stereo_cam_tilt_link"/>
    <origin rpy="0 0 ${pi/2}" xyz="0.065 0 0.004"/>
    <limit effort="10" lower="-0.08722" upper="1.62333" velocity="0.013"/>
  </joint>
  <link name="stereo_cam_tilt_link">
    <visual>
      <geometry>
        <box size="0.05 0.15 0.025" />
      </geometry>
      <origin rpy="0 0 ${-pi/2}" xyz="0.0 0.0 0.0"/>
      <xacro:insert_block name="material_orange" />
    </visual>
  </link>
  
  <!-- ************************ TeleCam ************************* -->
  <joint name="tele_cam_tilt" type="revolute">
    <parent link="head_link"/>
    <child link="tele_cam_tilt_link"/>
    <origin rpy="0 0 ${pi/2}" xyz="0.0325 0.0 0.3145"/>
    <limit effort="10" lower="-0.5957" upper="0.5957" velocity="0.013"/>
  </joint>
  <link name="tele_cam_tilt_link"></link>
  <joint name="tele_cam_pan" type="revolute">
    <parent link="tele_cam_tilt_link"/>
    <child link="tele_cam_pan_link"/>
    <origin rpy="0 -${pi/2} 0" xyz="0.0 0.0 0.0"/>
    <limit effort="10" lower="-0.7888" upper="0.7888" velocity="0.013"/>
  </joint>
  <link name="tele_cam_pan_link">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.05" />
   	  </geometry>
      <origin rpy="-${pi/2} 0 ${pi/2}" xyz="0.0 0.0 0.0"/>
      <xacro:insert_block name="material_orange" />
    </visual>
  </link>

  <!-- ************************** Arm *************************** -->
  <joint name="torso_turn" type="revolute">
    <parent link="body_link"/>
    <child link="torso_turn_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-0.0873" upper="2.2689" velocity="0.5"/>
  </joint>
  <link name="torso_turn_link">
    <visual>
      <geometry>
        <mesh filename="package://korus_meta/resources/stl/torso_turn.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <origin xyz="0.0 0.0 0.172" rpy="0.0 0.0 ${pi/2}"/>
<!--       <geometry> -->
<!--         <cylinder length="0.4" radius="0.1"/> -->
<!--       </geometry> -->
<!--       <origin rpy="0 0 0" xyz="0 0 0.2"/> -->
      <xacro:insert_block name="material_white" />
    </visual>
  </link>
  
  <joint name="torso_lift_offset" type="fixed">
    <parent link="torso_turn_link"/>
    <child link="torso_lift_offset_link"/>
    <origin xyz="0 0 0.114" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>
  <link name="torso_lift_offset_link"></link>
  
  <joint name="torso_lift" type="prismatic">
    <parent link="torso_lift_offset_link"/>
    <child link="torso_lift_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="0.0" upper="0.16" velocity="0.5"/>
  </joint>  
  <link name="torso_lift_link">
    <visual>
      <geometry>
        <box size="0.2185 0.25 0.13"/>
      </geometry>
      <origin xyz="-0.10925 0.0 0.0" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
  </link>

  <joint name="shoulder_pitch" type="revolute">
    <parent link="torso_lift_link"/>
    <child link="shoulder_pitch_link"/>
    <origin xyz="-0.241 -0.076 0" rpy="${pi} ${pi/2} 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-1.7453" upper="2.618" velocity="0.5"/>
  </joint>    
  <link name="shoulder_pitch_link">
    <visual>
      <geometry>
        <box size="0.270 0.093 0.055"/>
      </geometry>
      <origin xyz="0.081 0.0 0.0" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
  </link>

  <joint name="elbow_pitch" type="revolute">
    <parent link="shoulder_pitch_link"/>
    <child link="elbow_pitch_link"/>
    <origin xyz="0.162 0.0 0.045" rpy="0 0 3.141592"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-3.1416" upper="0.05" velocity="0.5"/>
  </joint>  
  <link name="elbow_pitch_link">
    <visual>
      <geometry>
        <box size="0.380 0.093 0.053"/>
      </geometry>
      <origin xyz="0.134 0.00 0.009" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>  
  </link>

  <joint name="wrist_pitch" type="revolute">
    <parent link="elbow_pitch_link"/>
    <child link="wrist_pitch_link"/>
    <origin xyz="0.268 0.0 -0.058" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-1.7453" upper="1.7453" velocity="0.5"/>
  </joint>
  <link name="wrist_pitch_link">
    <visual>
      <geometry>
        <box size="0.09 0.09 0.081"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>  
  </link>
  
  <joint name="wrist_roll" type="revolute">
    <parent link="wrist_pitch_link"/>
    <child link="wrist_roll_link"/>
    <origin xyz="0.045 0 0" rpy="0 ${pi/2} 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-2.0944" upper="2.0944" velocity="0.5"/>
  </joint>
  <link name="wrist_roll_link">
    <visual>
      <geometry>
        <box size="0.068 0.068 0.068"/>
      </geometry>
      <origin xyz="0.0 0.0 0.034" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
  </link>

  <joint name="wrist_yaw" type="revolute">
    <parent link="wrist_roll_link"/>
    <child link="wrist_yaw_link"/>
    <origin xyz="0 0 0.034" rpy="${pi/2} -${pi/2} 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-1.6232" upper="1.6232" velocity="0.5"/>
  </joint>
  <link name="wrist_yaw_link">
      <visual>
      <geometry>
        <box size="0.148 0.09 0.09"/>
      </geometry>
      <origin xyz="0.057 0.0 0.0" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>  
  </link>

  <!-- Endeffector (gripper) --> 
  <joint name="gripper_palm" type="fixed">
    <parent link="wrist_yaw_link"/>
    <child link="gripper_palm_link"/>
    <origin xyz="0.131 0 0" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-1.6232" upper="1.6232" velocity="0.5"/>
  </joint>
  <link name="gripper_palm_link">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <xacro:insert_block name="material_green"/>
    </visual> 
  </link>
  
  <!-- center of the volume covered by the fingers -->
  <joint name="gripper_center" type="fixed">
    <parent link="wrist_yaw_link"/>
    <child link="gripper_center_link"/>
    <origin xyz="0.161 0 0" rpy="0 0 0"/>
  </joint>
  <link name="gripper_center_link">
    <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <xacro:insert_block name="material_orange" />
    </visual>   
  </link>
  
  <joint name="gripper_tip" type="fixed">
    <parent link="wrist_yaw_link"/>
    <child link="gripper_tip_link"/>
    <origin xyz="0.191 0 0" rpy="0.0 0.0 0.0"/>
  </joint>
  <link name="gripper_tip_link">
      <visual>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <xacro:insert_block name="material_red" />
    </visual>  
  </link>
  
  <joint name="gripper_one_finger" type="revolute">
    <parent link="wrist_yaw_link"/>
    <child link="gripper_one_finger_tip_link"/>
    <origin xyz="0.131 -0.04 0.0" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-1.22173" upper="1.22173" velocity="0.5"/>
  </joint>
  <link name="gripper_one_finger_tip_link">
      <visual>
      <geometry>
        <box size="0.10 0.06 0.01"/>
      </geometry>
      <origin xyz="0.03 0.0 0.0" rpy="${pi/2} 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>  
  </link>

  <joint name="gripper_two_fingers" type="revolute">
    <parent link="wrist_yaw_link"/>
    <child link="gripper_two_fingers_tip_link"/>
    <origin xyz="0.131 0.04 0.0" rpy="${pi} 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-1.22173" upper="1.22173" velocity="0.5"/>
  </joint>
  <link name="gripper_two_fingers_tip_link">
      <visual>
      <geometry>
        <box size="0.10 0.06 0.01"/>
      </geometry>
      <origin xyz="0.03 0.0 0.0" rpy="${pi/2} 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>  
  </link>

</robot>