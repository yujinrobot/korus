<?xml version="1.0" ?>
  <robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  
    <!-- ************************ Properties ************************* -->
    <xacro:property name="pi" value="3.1415926535897931" />
    <xacro:property name="material_white">
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </xacro:property>
    <xacro:property name="material_orange">
      <material name="orange">
        <color rgba="1.0 0.7 0.0 1"/>
      </material>
    </xacro:property>
    <xacro:property name="material_red">
      <material name="red">
        <color rgba="1.0 0.0 0.0 1"/>
      </material>
    </xacro:property>
    <xacro:property name="material_grey_blue">
      <material name="grey_blue">
        <color rgba="0.4 0.4 1.0 1"/>
      </material>
    </xacro:property>
    <xacro:property name="material_blue">
      <material name="blue">
        <color rgba="0.0 0.0 1.0 1"/>
      </material>
    </xacro:property>
    <xacro:property name="material_green">
      <material name="green">
        <color rgba="0 .8 0 1"/>
      </material>
    </xacro:property>
    <xacro:property name="material_grey">
      <material name="grey">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </xacro:property>
    <xacro:property name="material_black">
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </xacro:property>
  
  <!-- ************************ Arm macro ************************* -->
  <xacro:macro name="jaco_arm" params="parent">
  <joint name="shoulder_roll" type="revolute">
    <parent link="${parent}"/>
    <child link="shoulder_roll_link"/>
    <origin xyz="-0.06 -0.0 0" rpy="${pi} ${pi/2} 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-${3*pi}" upper="${3*pi}" velocity="0.84"/>
  </joint>    
  <link name="shoulder_roll_link">
     <visual>
      <geometry>
        <cylinder length="0.2725" radius="0.04"/>
      </geometry>
      <origin xyz="0.0 0.0 0.13625" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2725" radius="0.04"/>
      </geometry>
      <origin xyz="0.0 0.0 0.13625" rpy="0 0 0"/>
    </collision>  
  </link>

  <joint name="shoulder_pitch" type="revolute">
    <parent link="shoulder_roll_link"/>
    <child link="shoulder_pitch_link"/>
    <origin xyz="0.0 0.0 0.2725" rpy="-${pi/2} ${pi/2} 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="0.73" upper="5.55" velocity="0.84"/>
  </joint>    
  <link name="shoulder_pitch_link">
    <visual>
      <geometry>
        <cylinder length="0.410" radius="0.03"/>
      </geometry>
      <origin xyz="0.205 0.0 -0.03" rpy="0 ${pi/2} 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.410" radius="0.03"/>
      </geometry>
      <origin xyz="0.205 0.0 -0.03" rpy="0 ${pi/2} 0"/>
    </collision>      
  </link>

  <joint name="elbow_pitch" type="revolute">
    <parent link="shoulder_pitch_link"/>
    <child link="elbow_pitch_link"/>
    <origin xyz="0.410 0.0 0.0113" rpy="0 0 ${pi}"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="0.3" upper="6.0" velocity="0.84"/>
  </joint>  
  <link name="elbow_pitch_link">
    <visual>
      <geometry>
        <cylinder length="0.2073" radius="0.03"/>
      </geometry>
      <origin xyz="0.10365 0.0 0.0" rpy="0 ${pi/2} 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2073" radius="0.03"/>
      </geometry>
      <origin xyz="0.10365 0.0 0.0" rpy="0 ${pi/2} 0"/>
    </collision>      
  </link>

  <joint name="wrist_pitch" type="revolute">
    <parent link="elbow_pitch_link"/>
    <child link="wrist_pitch_link"/>
    <origin xyz="0.2073 0.0 0.0" rpy="0.0 -${pi/2} -${pi}"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-${3*pi}" upper="${3*pi}" velocity="0.84"/>
  </joint>
  <link name="wrist_pitch_link">
  </link>

  <joint name="wrist_pitch_intermediate" type="fixed">
    <parent link="wrist_pitch_link"/>
    <child link="wrist_pitch_intermediate_link"/>
    <origin xyz="0.0 0.0 0.0" rpy="0 0.96 0"/>
  </joint>
  <link name="wrist_pitch_intermediate_link">
    <visual>
      <geometry>
        <cylinder length="0.074" radius="0.03"/>
      </geometry>
      <origin xyz="0.0 0.0 0.037" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.074" radius="0.03"/>
      </geometry>
      <origin xyz="0.0 0.0 0.037" rpy="0 0 0"/>
    </collision>       
  </link>
  
  <joint name="wrist_roll" type="revolute">
    <parent link="wrist_pitch_intermediate_link"/>
    <child link="wrist_roll_link"/>
    <origin xyz="0.0 0.0 0.074" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-${3*pi}" upper="${3*pi}" velocity="0.84"/>    
  </joint>
  <link name="wrist_roll_link">
    <visual>  
      <geometry>
        <cylinder length="0.074" radius="0.03"/>
      </geometry>
      <origin xyz="0.0 0.0 0.037" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.074" radius="0.03"/>
      </geometry>
      <origin xyz="0.0 0.0 0.037" rpy="0 0 0"/>
    </collision>      
  </link>

  <joint name="wrist_yaw" type="revolute">
    <parent link="wrist_roll_link"/>
    <child link="wrist_yaw_link"/>
    <origin xyz="0 0 0.074" rpy="0.0 -0.96 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-${3*pi}" upper="${3*pi}" velocity="0.84"/>
  </joint>
  <link name="wrist_yaw_link">
    <visual>  
      <geometry>
        <cylinder length="0.094" radius="0.04"/>
      </geometry>
      <origin xyz="0.0 0.0 0.047" rpy="0 0 0"/>
      <xacro:insert_block name="material_grey_blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.094" radius="0.04"/>
      </geometry>
      <origin xyz="0.0 0.0 0.047" rpy="0 0 0"/>
    </collision>        
  </link>

<!--   Endeffector (gripper)  -->
  <joint name="gripper_palm" type="fixed">
    <parent link="wrist_yaw_link"/>
    <child link="gripper_palm_link"/>
    <origin xyz="0.0 0 0.094" rpy="0.0 0.0 0.0"/>
  </joint>
  <link name="gripper_palm_link">
  </link>
  
  <!-- center of the volume covered by the fingers -->
<!--   <joint name="gripper_center" type="fixed"> -->
<!--     <parent link="wrist_yaw_link"/> -->
<!--     <child link="gripper_center_link"/> -->
